<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	<Authors>huseyin yazici</Authors>
	<Company>huseyinyazici</Company>
	<PackageId>AutoWorker</PackageId>
	<Version>1.0.0</Version>
	<PackageDescription>A lightweight AutoWorker and PartitionedAutoWorker for sequential and partitioned background job processing.</PackageDescription>
	<RepositoryUrl>https://github.com/huseyinyazicii/AutoWorker</RepositoryUrl>
	<PackageProjectUrl>https://github.com/huseyinyazicii/AutoWorker</PackageProjectUrl>
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
</Project>
